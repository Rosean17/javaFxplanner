<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="com.example.studyplanner.controller.ToDoController">
   <children>
      <TextField fx:id="taskField" prefWidth="500.0" promptText="Enter a new task..." />
      <Button fx:id="addButton" mnemonicParsing="false" onAction="#handleAddTask" text="Add Task" />
      <ListView fx:id="taskListView" prefHeight="250.0" prefWidth="500.0" />
      <HBox alignment="CENTER" prefWidth="500.0" spacing="10.0" />
      <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#handleDeleteTask" text="Delete Task" />
      <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#handleLogout" text="Logout" />
   </children>
</VBox>
